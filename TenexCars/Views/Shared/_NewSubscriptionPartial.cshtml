@model IEnumerable<TenexCars.Models.ViewModels.NewSubscriptionViewModel>

@foreach (var ops in Model)
{
    <li>
        <img src="@ops.OperatorLogo" style="width:50px;height:50px;" asp-append-version="true"/>
        <a href="/Operator/OperatorVehicles/@ops.OperatorId">@ops.CompanyName</a>
    </li>
}
